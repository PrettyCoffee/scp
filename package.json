{
  "name": "nameless",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint ./src --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "validate": "npm run lint && npm run build",
    "prepare": "cozy-git-hooks install -q"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.0.7",
    "@dnd-kit/modifiers": "^6.0.1",
    "@emotion/css": "^11.10.5",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@floating-ui/react": "^0.18.0",
    "@radix-ui/react-accordion": "^1.1.0",
    "@radix-ui/react-separator": "^1.0.1",
    "@radix-ui/react-toggle": "^1.0.1",
    "color": "^4.2.3",
    "jotai": "^2.0.2",
    "jotai-devtools": "^0.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.10.5",
    "@pretty-cozy/eslint-config": "^0.0.4",
    "@pretty-cozy/git-hooks": "^0.0.4",
    "@types/color": "^3.0.3",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@vitejs/plugin-react": "^2.2.0",
    "eslint": "^8.37.0",
    "typescript": "^4.8.4",
    "unplugin-auto-import": "^0.11.5",
    "vite": "^3.2.3"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "eslintConfig": {
    "extends": [
      "@pretty-cozy/eslint-config/base-ts",
      "@pretty-cozy/eslint-config/react"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint:fix"
    ],
    "*.{json,md,html}": [
      "prettier --write"
    ]
  }
}
